using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using IMES.Infrastructure.Repository._Metas;

namespace IMES.DataModel
{
    [ORMapping(typeof(ModelAssemblyCode))]
    [Serializable]
    public class ModelAssemblyCodeInfo
    {
        [ORMapping(ModelAssemblyCode.fn_model)]
        public string Model = null;

        [ORMapping(ModelAssemblyCode.fn_assemblyCode)]
        public string AssemblyCode = null;

        [ORMapping(ModelAssemblyCode.fn_revision)]
        public string Revision = null;

        [ORMapping(ModelAssemblyCode.fn_supplierCode)]
        public string SupplierCode = null;

        [ORMapping(ModelAssemblyCode.fn_remark)]
        public string Remark = null;


        [ORMapping(ModelAssemblyCode.fn_editor)]
        public String Editor = null;

        [ORMapping(ModelAssemblyCode.fn_cdt)]
        public DateTime Cdt = DateTime.MinValue;

        [ORMapping(ModelAssemblyCode.fn_udt)]
        public DateTime Udt = DateTime.MinValue;      
    }
}
