using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using IMES.Infrastructure.Repository._Metas;

namespace IMES.DataModel
{
    [Serializable]
    [ORMapping(typeof(Doalist))]
    public class DOAListInfo
    {
        [ORMapping(Doalist.fn_cdt)]
        public DateTime cdt = DateTime.MinValue;
        [ORMapping(Doalist.fn_editor)]
        public String editor = null;
        [ORMapping(Doalist.fn_id)]
        public Int32 id = int.MinValue;
        [ORMapping(Doalist.fn_mark)]
        public Int32 mark = int.MinValue;
        [ORMapping(Doalist.fn_pno)]
        public String pno = null;
        [ORMapping(Doalist.fn_poNo)]
        public String poNo = null;
        [ORMapping(Doalist.fn_rma)]
        public String rma = null;
        [ORMapping(Doalist.fn_sno)]
        public String sno = null;
        [ORMapping(Doalist.fn_tp)]
        public String tp = null;
        [ORMapping(Doalist.fn_udt)]
        public DateTime udt = DateTime.MinValue;
    }

    [Serializable]
    [ORMapping(typeof(DOAMBList))]
    public class DOAMBListInfo
    {
        [ORMapping(DOAMBList.fn_cdt)]
        public DateTime cdt = DateTime.MinValue;
        [ORMapping(DOAMBList.fn_editor)]
        public String editor = null;
        [ORMapping(DOAMBList.fn_groupNo)]
        public String groupNo = null;
        [ORMapping(DOAMBList.fn_message)]
        public String message = null;
        [ORMapping(DOAMBList.fn_pcbno)]
        public String pcbno = null;
        [ORMapping(DOAMBList.fn_status)]
        public String status = null;
    }
}
