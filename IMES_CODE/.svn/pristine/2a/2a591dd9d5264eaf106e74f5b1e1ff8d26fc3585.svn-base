using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using IMES.Infrastructure.Repository._Metas;

namespace IMES.DataModel
{
    [ORMapping(typeof(FAIModel))]
    [Serializable]
    public class FAIModelInfo
    {
        [ORMapping(FAIModel.fn_model)]
        public String Model = null;

        [ORMapping(FAIModel.fn_modelType)]
        public String ModelType = null;

        [ORMapping(FAIModel.fn_customSKU)]
        public String CustomSKU = null;

        [ORMapping(FAIModel.fn_skucode)]
        public String SKUCode = null;

        [ORMapping(FAIModel.fn_skusuffix)]
        public String SKUSuffix = null;

        [ORMapping(FAIModel.fn_planInputDate)]
        public DateTime PlanInputDate = DateTime.MinValue;

        [ORMapping(FAIModel.fn_faqty)]
        public int FAQty = int.MinValue;

        [ORMapping(FAIModel.fn_inFAQty)]
        public int inFAQty = int.MinValue;

        [ORMapping(FAIModel.fn_pakqty)]
        public int PAKQty = int.MinValue;

        [ORMapping(FAIModel.fn_inPAKQty)]
        public int inPAKQty = int.MinValue;

        [ORMapping(FAIModel.fn_pakstartdate)]
        public DateTime PAKStartDate = DateTime.MinValue;

        [ORMapping(FAIModel.fn_fastate)]
        public String FAState = null;

        [ORMapping(FAIModel.fn_pakstate)]
        public String PAKState = null;

        [ORMapping(FAIModel.fn_remark)]
        public String Remark = null;

        [ORMapping(FAIModel.fn_editor)]
        public String Editor = null;

        [ORMapping(FAIModel.fn_cdt)]
        public DateTime Cdt = DateTime.MinValue;

        [ORMapping(FAIModel.fn_udt)]
        public DateTime Udt = DateTime.MinValue;      
    }
}
