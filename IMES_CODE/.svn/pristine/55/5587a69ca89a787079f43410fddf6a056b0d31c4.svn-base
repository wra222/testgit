       });
       
       function getUrlParam(name) {
           var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
           var r = window.location.search.substr(1).match(reg);
           if (r != null) return unescape(r[2]); return null;
       }

        $(function() {


            $('#btnSelected').click(function() {
                var begtime = $("#act_start_time").val();
                var endtime = $("#act_stop_time").val();
               
                var process = $("#DLProcess").val();

                if (begtime > endtime) {
                    alert("开始时间不能大于结束时间!");
                    return false;
                }
                

                $('#reportTable').bootstrapTable('refresh', {
                url: '../../Service/Commn_GetUPHDara.ashx?TYPE=Efficiency_Process_Hour&Process=' + process  + '&BeginTime=' + begtime + '&EndTime=' + endtime + ''
                });

                $("#tanchuang").hide();
            });
            var d = new Date();
            var nowtime = d.getFullYear() + "-" + (d.getMonth() + 1) + "-" + (d.getDate());
            var urlprocess = getUrlParam("Process");

            $('#reportTable').bootstrapTable({
                dataType: 'JSON',
                method: 'get',
                url: '../../Service/Commn_GetUPHDara.ashx?TYPE=Efficiency_Process_Hour&Process=' + urlprocess + '&BeginTime=' + nowtime + '&EndTime=' + nowtime + '',

                cache: false,
                height: 500,
                striped: true,
                pagination: true,
                pageSize: 20,
                pageNumber: 1,
                pageList: [20, 50, 100, 150, 249, 5000],
                search: true,
                showToggle: true,
                showColumns: true,
                showRefresh: true,
                showExport: true,
                exportTypes: ['excel'],
                search: true,
                clickToSelect: true,
                columns: [
                   { field: "Dt", title: "日期", align: "center", valign: "middle", sortable: "true" },
                   { field: "Process", title: "制程", align: "center", valign: "middle", sortable: "true" },
                   { field: "Hour", title: "时间", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value.trim() + ':00~' + (parseInt(value) + 1) + ':00'; } },
                   { field: "Attend_normal", title: "应出勤", align: "center", valign: "middle", sortable: "true" },
                    { field: "Attend_OA", title: "实际出勤人力", align: "center", valign: "middle", sortable: "true" },
                   { field: "Attend", title: "实际出勤工时", align: "center", valign: "middle", sortable: "true" ,visible:false},
                   { field: "Qty", title: "产出", align: "center", valign: "middle", sortable: "true" },
                   { field: "Cap_ST", title: "满载工时", align: "center", valign: "middle", sortable: "true" },
                   { field: "Out_ST", title: "产出工时", align: "center", valign: "middle", sortable: "true" },
                   { field: "In_ST", title: "投入工时", align: "center", valign: "middle", sortable: "true" },
                   { field: "PerST", title: "开线率（%）", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value + '%'; }, cellStyle: function(value) { if (parseInt(value) < 90) return { classes: 'tips' }; else return {} } },
                   { field: "PerEff", title: "生产效率（%）", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value + '%'; }, cellStyle: function(value) { if (parseInt(value) < 90) return { classes: 'tips' }; else return {} } },
                   { field: "PerOut", title: "整体产出率（%）", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value + '%'; }, cellStyle: function(value) { if (parseInt(value) < 90) return { classes: 'tips' }; else return {} } },
                   { field: "PCQty", title: "排单数", align: "center", valign: "middle", sortable: "true" }
                  ],
                onPageChange: function(size, number) {
                },
                formatNoMatches: function() {
                    return 'No Data！';
                }
            });
            
          
     
        });
