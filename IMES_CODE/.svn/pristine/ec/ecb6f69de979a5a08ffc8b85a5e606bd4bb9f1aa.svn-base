using System.Collections.Generic;

namespace IMES.CheckItemModule.Utility
{
    public class QtyParts
    {
        int _qty;
        IList<IMES.FisObject.Common.Part.IPart> _parts;

        public QtyParts(int qty, IList<IMES.FisObject.Common.Part.IPart> parts)
        {
            _qty = qty;
            _parts = parts;
        }

        public IList<IMES.FisObject.Common.Part.IPart> Parts
        {
            get { return _parts; }
            set { _parts = value; }
        }

        public int Qty
        {
            get { return _qty; }
            set { _qty = value; }
        }
    }
}
