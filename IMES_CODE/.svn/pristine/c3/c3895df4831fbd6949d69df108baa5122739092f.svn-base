<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="Archive"    type="ArchiveDB.Config,ArchiveDB"/>
  </configSections>
  <appSettings>
    <add key="IsDeleteSrcData" value="N"/>
    <add key="LogPath" value="G:\ArchiveAP\Log\"/>
    <add key="XMLPath" value="G:\ArchiveAP\XML\"/>
    <!--設置刪除資料的天數 :  Shipdate<Day... 不可小於90-->
    <add key="Day" value="30"/>
    <!--設置刪除資料的Cdt天數 :  Cdt<Day...不可小於90-->
    <add key="ContinueDeleteDay" value="0"/>
    <add key="DBName" value="HPIMES"/>
    <add key="EnableMail" value="Y"/>
    <add key="CdtDay" value="200"/>
    <add key="DeleteRows" value="500"/>
    <add key="SelepTime" value="00:00:00.1"/>
    <add key="DeleteTimeOut" value="6000"/>

    <!--設置保存log天數 :  Cdt<Day...-->
    <add key="KeepLogDay" value="7"/>
    <!-- 設定Delete Data時,取得Time Stamp的間隔,單位min-->
    <add key="DeleteTimeStampInterval" value="30"/>
    <add key="IsTest" value="N"/>
    <add key="SMTP" value="ics-mx03.ies.inventec"/>
    <!--設置Mail收件人,以,區隔-->
    <add key="MailTo" value="He.Jia-you@inventec.com"/>


  </appSettings>
