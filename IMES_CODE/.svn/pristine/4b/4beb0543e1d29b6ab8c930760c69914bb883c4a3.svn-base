       function ClearAll() {
           $("#act_start_time").val("");
           $("#act_stop_time").val("");
           $("#DLPdline").val("");
          
        }

        $(function() {


            $('#btnSelected').click(function() {
                var begtime = $("#act_start_time").val();
                var endtime = $("#act_stop_time").val();
                var pdline = $("#DLPdline").val();
                var process = $("#DLProcess").val();
                
                if (begtime > endtime) {
                    alert("开始时间不能大于结束时间!");
                    return false;
                }

                $('#reportTable').bootstrapTable('refresh', {
                    url: '../../Service/Commn_GetUPHDara.ashx?TYPE=Efficiency_Hour&Process=' + process + '&Line=' + pdline + '&BeginTime=' + begtime + '&EndTime=' + endtime + ''
                });

                $("#tanchuang").hide();
            });


            $('#reportTable').bootstrapTable({
                dataType: 'JSON',
                method: 'get',
                //url: '../Quality_Management/ajax/GetRepair_Analyze.aspx?type=getData&table=info',
                cache: false,
                height: 380,
                striped: true,
                pagination: true,
                pageSize: 20,
                pageNumber: 1,
                pageList: [20, 50, 100, 150, 249, 5000],
                search: true,
                showToggle: true,
                showColumns: true,
                showRefresh: true,
                showExport: true,
                exportTypes: ['excel'],
                search: true,
                clickToSelect: true,
                columns: [
                  { field: "Dt", title: "日期", align: "center", valign: "middle", sortable: "true" },
                   { field: "Hour", title: "小时", align: "center", valign: "middle", sortable: "true" },
                  { field: "Attend_normal", title: "应出勤", align: "center", valign: "middle", sortable: "true" },
                  { field: "Attend", title: "实际出勤", align: "center", valign: "middle", sortable: "true" },
                  { field: "PdLine", title: "线别", align: "center", valign: "middle", sortable: "true" },
                  { field: "Family", title: "系列", align: "center", valign: "middle", sortable: "true" },
                  { field: "Process", title: "制程", align: "center", valign: "middle", sortable: "true" },
                  { field: "Station", title: "站点", align: "center", valign: "middle", sortable: "true" },
                  { field: "Qty", title: "产出", align: "center", valign: "middle", sortable: "true" },
                  { field: "ST", title: "单台工时", align: "center", valign: "middle", sortable: "true" },
                  { field: "Cycle", title: "线速", align: "center", valign: "middle", sortable: "true" },
                  { field: "NormalUPH", title: "标准UPH", align: "center", valign: "middle", sortable: "true" },
                  { field: "RealUPH", title: "实际UPH", align: "center", valign: "middle", sortable: "true" },
                  { field: "Efficiency", title: "生产效率（%）", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value + '%'; }, cellStyle: function(value) { if (parseInt(value) < 90) return { classes: 'tips' }; else return {} } },
                  { field: "UPH_achieve", title: "UPH达成率（%）", align: "center", valign: "middle", sortable: "true", formatter: function(value) { return value + '%'; }, cellStyle: function(value) { if (parseInt(value) < 90) return { classes: 'tips' }; else return {} } }
                  
                  ],
                onPageChange: function(size, number) {
                },
                formatNoMatches: function() {
                    return 'No Data！';
                }
            });
            $("#DLProcess").change(function() {

                var process = $('#DLProcess').children('option:selected').val();
                if (typeof (process) == "undefined" || process == "") {
                    $("#DLPdline").empty();
                } else {
                    $("#DLPdline").empty();

                    $.getJSON('../../Service/Commn_GetUPHDara.ashx?TYPE=GetPdline&Process=' + process, function(data) {
                        var option = "";
                        $.each(data, function(i, item) { option += "<option value=\"" + item.Line + "\">" + item.Line + "</option>"; });
                        $("#DLPdline").append(option);
                    });
                }
            });
        });
        
