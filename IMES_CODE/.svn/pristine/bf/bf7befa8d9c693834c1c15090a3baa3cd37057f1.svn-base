using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using IMES.Infrastructure.Repository._Metas;

namespace IMES.DataModel
{
    [ORMapping(typeof(Hp_Grade))]
    [Serializable]
    public class GradeInfo
    {
        [ORMapping(Hp_Grade.fn_cdt)]
        public DateTime cdt = DateTime.MinValue;
        [ORMapping(Hp_Grade.fn_editor)]
        public String editor = null;
        [ORMapping(Hp_Grade.fn_energia)]
        public String energia = null;
        [ORMapping(Hp_Grade.fn_espera)]
        public String espera = null;
        [ORMapping(Hp_Grade.fn_family)]
        public String family = null;
        [ORMapping(Hp_Grade.fn_grade)]
        public String grade = null;
        [ORMapping(Hp_Grade.fn_id)]
        public Int32 id = int.MinValue;
        [ORMapping(Hp_Grade.fn_series)]
        public String series = null;
        [ORMapping(Hp_Grade.fn_udt)]
        public DateTime udt = DateTime.MinValue;
    }
}
